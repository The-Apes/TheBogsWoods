      line: "   \r\r\r\n"
      right: 0
      soundEffect: {fileID: 8300000, guid: b6b38cae49566404395b786b72d16387, type: 3}
      customEvent: cg11
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: Otto was no longer... Otto.
      right: 0
      soundEffect: {fileID: 0}
      customEvent: black
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
