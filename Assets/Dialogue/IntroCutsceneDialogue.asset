  dialogue:
    dialogueLines:
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "Once upon a time. a sister and brother, Ruri and Otto, played in their
        backyard.\r"
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg0
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "Until Otto\u2019s ball bounced away... swallowed by the forest.\r"
      right: 0
      soundEffect: {fileID: 8300000, guid: e1ade092005d08e44bb7811111d90a27, type: 3}
      customEvent: cg1
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: They searched the woods, high and low... but the ball was gone.
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg2
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "Instead, they found a shrine\u2014cracked, overgrown, forgotten.\n\n\n"
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg3
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: 'Otto stepped forward, poked at the shrine, laughing.

'
      right: 0
      soundEffect: {fileID: 0}
      customEvent: 
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: But the shrine did not laugh back.
      right: 0
      soundEffect: {fileID: 0}
      customEvent: 
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: It stirred.
      right: 0
      soundEffect: {fileID: 8300000, guid: 6a5e2ce60352cf941adb69fe4df997fd, type: 3}
      customEvent: cg4
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: 'Everything around the siblings began to float, including them.


'
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg8
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "The air grew heavy.\r\nThe trees twisted.\r\nAnd the world\u2014\r\nfolded
        in on itself.\n\n"
      right: 0
      soundEffect: {fileID: 8300000, guid: a14995eff8041454d84ccc6d4ea38a9b, type: 3}
      customEvent: cg9
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: 'In a blink... they were somewhere else.

        A forest, bathed in
        twilight.'
      right: 0
      soundEffect: {fileID: 0}
      customEvent: black
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: ' '
      right: 0
      soundEffect: {fileID: 8300000, guid: b777be2d26e6b3247a2d942c86ddcac3, type: 3}
      customEvent: cg12
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: Soon after the shrine transported them, it started to rumble.
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg4
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: A dark substance oozed from the shrine's crevaces
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg5
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: ' '
      right: 0
      soundEffect: {fileID: 8300000, guid: 927770d6919099841bc32902490eadab, type: 3}
      customEvent: black
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: 'From within, something emerged.

'
      right: 0
      soundEffect: {fileID: 0}
      customEvent: 
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: 'A giant. Still. Watching.

'
      right: 0
      soundEffect: {fileID: 8300000, guid: 6a5e2ce60352cf941adb69fe4df997fd, type: 3}
      customEvent: cg6
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "Otto mocked the giant, muttering insults \u2014 \u201CStupid. Creepy.\u201D"
      right: 0
      soundEffect: {fileID: 0}
      customEvent: cg7
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
        frequency: 0
        volume: 0
        pitch: 0
      line: "The giant raised its hand. A curse passed through the air like wind.
        \r\r\r\n"
      right: 0
      soundEffect: {fileID: 8300000, guid: 6dd9ce6d8af4c754d99efc216bbdefa7, type: 3}
      customEvent: cg10
    - character: {fileID: 11400000, guid: 78be226473a7fd544b2afd5a21a55286, type: 2}
      characterOverride:
        characterName: 
        characterSprite: {fileID: 0}
        color: {r: 0, g: 0, b: 0, a: 0}
        blipSound: []
